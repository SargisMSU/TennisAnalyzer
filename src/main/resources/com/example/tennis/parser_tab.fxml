<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Tab fx:id="liveTab" text="Парсер" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.example.tennis.fx.ParserController">
  <VBox prefWidth="-1.0">
    <HBox VBox.vgrow="NEVER">
      <VBox HBox.hgrow="ALWAYS">
        <HBox>
          <Label alignment="CENTER" contentDisplay="CENTER" text="Ссылка:" HBox.hgrow="ALWAYS">
            <HBox.margin>
              <Insets top="5.0" />
            </HBox.margin>
          </Label>
          <TextField fx:id="linkTA" promptText="пример:https://www.flashscore.com/tennis/challenger-men-singles/biella-7/results/" HBox.hgrow="ALWAYS">
            <HBox.margin>
              <Insets right="15.0" />
            </HBox.margin>
          </TextField>
        </HBox>
        <HBox.margin>
          <Insets left="3.0" />
        </HBox.margin>
      </VBox>
      <VBox.margin>
        <Insets top="5.0" />
      </VBox.margin>
    </HBox>
    <HBox id="HBox" alignment="CENTER" spacing="5.0" VBox.vgrow="NEVER">
      <VBox.margin>
        <Insets bottom="5.0" top="5.0" />
      </VBox.margin>
      <Button fx:id="onParseButton" contentDisplay="CENTER" onAction="#onParseButtonClick" text="Старт" />
    </HBox>
    <HBox id="HBox" alignment="CENTER" spacing="5.0" VBox.vgrow="ALWAYS">
      <VBox.margin>
        <Insets bottom="5.0" top="5.0" />
      </VBox.margin>
      <TextArea fx:id="logsTA" editable="false" prefWidth="-1.0" visible="true" wrapText="true" HBox.hgrow="ALWAYS" />
    </HBox>
  </VBox>
</Tab>
